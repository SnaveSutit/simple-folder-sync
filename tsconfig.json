{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "ES2022",
		"target": "ES2022",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"strict": true,
		"types": ["node"],
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"allowUmdGlobalAccess": true
		// "declaration": true,
		// "emitDeclarationOnly": true
	}
}
